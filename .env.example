# Sora API Configuration
# Copy this file to .env and fill in your actual values

# Your Sora API Key (required)
# Get your API key from https://api.dzz.ai
SORA_API_KEY=your_api_key_here

# Sora API Base URL (optional)
# Default: https://api.dzz.ai
SORA_BASE_URL=https://api.dzz.ai

# Flask Configuration (optional)
FLASK_ENV=development
FLASK_DEBUG=True
