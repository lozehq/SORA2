{
  "functions": {
    "api/*": {
      "runtime": "nodejs22.x",
      "handler": "index.js"
    }
  },
  "build": {
    "command": "",
    "output": "."
  },
  "routes": [
    {
      "src": "/api/health",
      "dest": "/functions/api/health.js"
    },
    {
      "src": "/api/generate",
      "dest": "/functions/api/generate.js"
    }
  ]
}

